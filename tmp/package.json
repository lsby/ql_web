{
    "name": "ql_web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "node_start": "cross-env NODE_ENV=production PORT=80 node ./dist/service/main.js",
        "pm2_start": "pm2 set pm2-logrotate:max_size 10M && cross-env NODE_ENV=production PORT=80 pm2 start node -e ./logs/error.log -l ./logs/out.log --name ql_web ./dist/service/main.js",
        "pm2_restart": "pm2 restart ql_web",
        "pm2_stop": "pm2 stop ql_web",
        "pm2_rm": "npm run pm2_stop && pm2 delete ql_web",
        "pm2_list": "pm2 list",
        "pm2_log": "pm2 logs",
        "pm2_setlog": "pm2 set pm2-logrotate:max_size 10M",
        "docker_build": "docker build -t ql_web .",
        "docker_start": "docker-compose up -d --remove-orphans && npm run docker_log",
        "docker_stop": "docker-compose stop",
        "docker_log": "docker-compose logs -f --tail=\"100\"",
        "docker_rm": "docker-compose stop && docker-compose rm -f",
        "dev": "cross-env NODE_ENV=development PORT=3000 gulp",
        "web_build": "webpack --config ./web/webpack/webpack.prod.js",
        "web_analyz": "cross-env analyz=true npm run web_build",
        "service_build": "webpack --config ./service/webpack/webpack.prod.js",
        "service_analyz": "cross-env analyz=true npm run service_build"
    },
    "dependencies": {
        "connect-loki": "^1.1.0",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.6.4",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "get-url-params": "^1.0.1",
        "http-errors": "~1.6.3",
        "jquery": "^3.4.1",
        "morgan": "~1.9.1",
        "pm2": "^4.4.0",
        "request": "^2.88.2",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "view-design": "^4.2.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@babel/preset-env": "^7.10.3",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.5.3",
        "file-loader": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.2.0",
        "gulp-nodemon": "^2.5.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "nodemon": "^2.0.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.6",
        "url-loader": "^4.0.0",
        "vue-loader": "^15.9.3",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
