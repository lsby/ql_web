{
    "name": "ql_web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "npm run 开发模式",
        "node_启动": "cross-env NODE_ENV=production PORT=80 node -r esm ./app/main.js",
        "pm2_启动": "cross-env NODE_ENV=production PORT=80 pm2 set pm2-logrotate:max_size 10M && pm2 --node-args=\"-r esm\" start node -e ./logs/error.log -l ./logs/out.log --name ql_web ./app/main.js",
        "pm2_重启": "pm2 restart ql_web",
        "pm2_停止": "pm2 stop ql_web",
        "pm2_删除": "npm run pm2_停止 && pm2 delete ql_web",
        "pm2_列表": "pm2 list",
        "pm2_日志": "pm2 logs",
        "docker_编译": "docker build -t ql_web .",
        "docker_启动": "docker-compose up -d --remove-orphans && docker-compose logs -f --tail=\"100\"",
        "docker_停止": "docker-compose stop",
        "docker_日志": "docker-compose logs -f --tail=\"100\"",
        "docker_删除": "docker-compose stop && docker-compose rm -f",
        "开发模式": "cross-env NODE_ENV=development PORT=3000 gulp",
        "前端_编译": "webpack --config ./webpack/webpack.prod.js",
        "前端_分析": "cross-env NODE_ENV=development PORT=3000 analyz=true webpack --config ./webpack/webpack.prod.js",
        "后端_编译": "",
        "后端_分析": ""
    },
    "dependencies": {
        "connect-loki": "^1.1.0",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.6.4",
        "cross-env": "^7.0.2",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "file-loader": "^6.0.0",
        "get-url-params": "^1.0.1",
        "http-errors": "~1.6.3",
        "jquery": "^3.4.1",
        "morgan": "~1.9.1",
        "pm2": "^4.4.0",
        "request": "^2.88.2",
        "require-context": "^1.1.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "view-design": "^4.2.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.5.3",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.2.0",
        "gulp-nodemon": "^2.5.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "nodemon": "^2.0.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.1",
        "url-loader": "^4.0.0",
        "vue-loader": "^15.9.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
