{
  "name": "ql_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start_node": "node ./node_modules/cross-env/src/bin/cross-env NODE_ENV=production PORT=80 node -r esm ./app/main.js",
    "start": "node ./node_modules/cross-env/src/bin/cross-env NODE_ENV=production PORT=80 node ./node_modules/pm2/bin/pm2 --node-args=\"-r esm\" start node -e ./logs/error.log -l ./logs/out.log --name ql_web ./app/main.js",
    "restart": "node ./node_modules/pm2/bin/pm2 restart ql_web",
    "stop": "node ./node_modules/pm2/bin/pm2 stop ql_web",
    "delete": "node ./node_modules/pm2/bin/pm2 delete ql_web",
    "list": "node ./node_modules/pm2/bin/pm2 list",
    "logs": "node ./node_modules/pm2/bin/pm2 logs",
    "dev": "node ./node_modules/cross-env/src/bin/cross-env NODE_ENV=development PORT=3000 node ./node_modules/gulp/bin/gulp",
    "build": "node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.prod.js",
    "docker_build": "docker build -t ql_web .",
    "docker_start": "docker-compose up -d --remove-orphans && docker-compose logs -f --tail=\"100\"",
    "docker_stop": "docker-compose stop",
    "docker_log": "docker-compose logs -f --tail=\"100\"",
    "docker_rm": "docker-compose stop && docker-compose rm -f"
  },
  "dependencies": {
    "@lsby/ql_js": "^1.0.6",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "esm": "^3.2.25",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "morgan": "~1.9.1",
    "session-file-store": "^1.4.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-nodemon": "^2.5.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.3",
    "pm2": "^4.4.0",
    "popper.js": "^1.16.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^4.2.2"
  }
}
