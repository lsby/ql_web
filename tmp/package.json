{
  "name": "ql_web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx cross-env NODE_ENV=production PORT=80 npx pm2 start node -e ./logs/error.log -l ./logs/out.log --name ql_web -- ./app/main.mjs",
    "restart": "npx pm2 restart ql_web",
    "stop": "npx pm2 stop ql_web",
    "delete": "npx pm2 delete ql_web",
    "list": "npx pm2 list",
    "dev": "npx cross-env NODE_ENV=development PORT=3000 npx nodemon --inspect ./app/main.mjs",
    "web": "npx webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "npx webpack --config ./webpack/webpack.prod.js",
    "docker_build": "docker build -t ql_web .",
    "docker_start": "docker-compose up -d --remove-orphans && docker-compose logs -f --tail=\"100\"",
    "docker_stop": "docker-compose stop",
    "docker_log": "docker-compose logs -f --tail=\"100\"",
    "docker_rm": "docker-compose stop && docker-compose rm -f"
  },
  "dependencies": {
    "@lsby/node_ex": "^1.0.17",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "pm2": "^4.2.3",
    "session-file-store": "^1.4.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^7.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.0.4",
    "popper.js": "^1.16.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}